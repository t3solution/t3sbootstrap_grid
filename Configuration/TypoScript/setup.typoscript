plugin.tx_t3sbootstrap_grid {
	view {
		templateRootPaths.0 = EXT:t3sbootstrap_grid/Resources/Private/Templates/
		templateRootPaths.10 = {$plugin.tx_t3sbootstrap_grid.view.templateRootPath}
	}
}

page {
	includeCSS {
		bootstrapCDN = https://cdn.jsdelivr.net/npm/bootstrap@{$plugin.tx_t3sbootstrap_grid.settings.bootstrapVersion}/dist/css/bootstrap.min.css
		bootstrapCDN.external = 1
		bootstrapCDN.forceOnTop = 1
		bootstrapCDN.if.isTrue = {$plugin.tx_t3sbootstrap_grid.settings.enableCDN}
	}
}

